{
    "configurations": [
    {
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "name": "Launch index.html",
        "file": "${workspaceFolder}/index.html"
    },
    {
        "name": "Launch Program",
        "program": "${fileDirname}/${fileBasenameNoExtension}.ts",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "pwa-node"
    }
    ]
}